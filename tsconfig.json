{
  "compilerOptions": {
    "baseUrl": "./",
    "rootDir": "./",
    "outDir": "./lib",
    "target": "es6",
    "module": "commonjs",
    // "moduleResolution": "",
    "lib": ["ES2020", "dom"],
    "allowJs": true,
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["./lib"]
}
